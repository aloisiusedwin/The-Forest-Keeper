; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ProjectMN"
run/main_scene="res://Scenes/Main Menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://tfk-icon.png"
config/icon="res://tfk-icon.svg"
config/windows_native_icon="res://LOGOMARK.ico"

[autoload]

Global="*res://Scripts/global.gd"
SimpleGrass="*res://addons/simplegrasstextured/singleton.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={}
directories/dtl_directory={
"meno1": "res://models/npc/meno/meno1.dtl",
"meno2": "res://models/npc/meno/meno2.dtl",
"quest_start": "res://models/npc/lee/quest_start.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=[]
layout/default_style="Default"
variables={}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.05
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.5
text/advance_delay=0.5
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
text/autocolor_names=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/qodot/plugin.cfg", "res://addons/simplegrasstextured/plugin.cfg")

